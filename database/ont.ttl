@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix geo: <http://www.opengis.net/ont/geosparql#>.
@prefix xs: <http://www.w3.org/2001/XMLSchema#>.

# Our application specific namespace
@prefix ex: <http://openis.ititou.be/ont#>.

ex:Track a owl:Class ;
    rdfs:subclassOf geo:Feature .

ex:hasLength a owl:ObjectProperty ;
    rdfs:label "hasLength" ;
    rdfs:comment "The length, in meters, of a track" ;
    rdfs:range xs:integer ;
    rdfs:domain ex:Track .

ex:ExerciseSpot a owl:Class ;
    rdfs:subclassOf geo:Feature .

ex:isAlongTrack a owl:ObjectProperty ;
    rdfs:label "isAlongTrack" ;
    rdfs:comment "The track along which the exercise spot is located" ;
    rdfs:range ex:Track ;
    rdfs:domain ex:ExerciseSpot .
